<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @project.name %>
</p>

<p>
  <strong>Duedate:</strong>
  <%= @project.dueDate %>
</p>

<p>
  <strong>Budget:</strong>
  <%= number_to_currency(@project.budget) %>
</p>

<p>
  <strong>User:</strong>
  <%= @project.user.email %>
</p>
<%= link_to 'Edit Current Project', edit_project_path(@project) %>

<%= render 'dev_entries/form', dev_entry: @project_dev_entry %>
<div id="listing">
  <%= render 'dev_entries/updated_list', project: @project %>
</div>

<%= link_to 'Back to list of projects', projects_path %>
