<p id="notice"><%= notice %></p>

<strong>Current Entries for <%= @project.name %></strong>

<table>
  <thead>
    <tr>
      <th>Devdate</th>
      <th>Devduration</th>
      <th>Note</th>
      <th>Project</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @devEntriesOnProject.each do |dev_entry| %>
      <tr>
        <td><%= dev_entry.devDate %></td>
        <td><%= dev_entry.devDuration %></td>
        <td><%= dev_entry.note %></td>
        <td><%= dev_entry.project.name %></td>
        <td><%= dev_entry.user.email %></td>
        <td><%= link_to 'Edit', edit_dev_entry_path(dev_entry) %></td>
        <td><%= link_to 'Destroy', dev_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
